<h1>Editing feed</h1>
<fieldset>
  <legend>Data Feed</legend>
	<% form_for(@feed, :url => user_feed_path(@user, @feed)) do |f| %>
	  <%= render(:partial => "form", :object => @feed, :locals => {:f => f}) %>
	  <p style="text-align:center;clear:both;">
	    <%= f.submit "Update" %>
	  </p>
	<% end %>
</fieldset>
